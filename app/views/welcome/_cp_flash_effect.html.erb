<div id="da-thumbs" class="da-thumbs">
  <% @jobs.limit(12).each do |job| %>

  <div id="hahali" class="col-xs-2 show-item">
    <%= link_to job_path(job) do %>
    <% if job.logo.present? %>
    <%= image_tag(job.logo.thumb.url, class: "img-responsive") %>
  <% else %>
    <%= image_tag("http://placehold.it/200x200&text=No Pic") %>
    <% end %>
    <% end %>
  </div>
  <% end %>
</div>
